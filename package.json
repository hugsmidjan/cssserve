{
  "name": "cssserve",
  "version": "1.5.0",
  "dist_package_json": {
    "bin": {
      "cssserve": "server.js"
    },
    "scripts": {},
    "engines": {
      "node": ">=16"
    }
  },
  "engines": {
    "node": "^16"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "publishdist": "yarn run build  &&  cd dist  &&  npm publish",
    "publishcanary": "yarn run build  &&  cd dist  &&  npm publish --tag canary",
    "prepublishOnly": "echo \"\nRun 'yarn run publishdist' to publish this package\n\n\"  && exit 1",
    "format": "eslint --fix  \"*.js\" \"build/*.js\" \"src/**/*.{js,ts,tsx}\"  &&  prettier --write \"*.md\" \"*.json\"  \"src/**/*.json\"",
    "build": "yarn  &&  gulp build  &&  yarn run test",
    "dev": "yarn  &&  gulp dev",
    "test": "ospec 'testing/__tests/**/*.tests.js'",
    "dev:test": "yarn run test  &&  onchange 'testing/__tests/**/*.tests.js' -f add,change -- ospec {{changed}}",
    "dev:server": "node dist/server --config testing/cssserve-config.json"
  },
  "dependencies": {
    "@fastify/compress": "^6.1.1",
    "@fastify/cors": "^8.1.0",
    "@fastify/static": "^6.5.0",
    "compression": "^1.7.4",
    "fastify": "^4.5.3",
    "glob": "^8.0.3",
    "lru-cache": "^7.14.0",
    "rc": "^1.2.8"
  },
  "__devDependencies": {
    "@hugsmidjan/qj": "^4.10.2"
  },
  "devDependencies": {
    "@hugsmidjan/gulp-rollup": "^0.9.0",
    "@hugsmidjan/hxmstyle": "^6.2.0",
    "@types/compression": "^1.7.2",
    "@types/glob": "^8.0.0",
    "@types/lru-cache": "^7.10.10",
    "@types/node": "^16",
    "@types/ospec": "^4.0.4",
    "@types/rc": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "del": "^6.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-destructure-depth": "^1.0.3",
    "eslint-plugin-destructuring": "^2.2.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-total-functions": "^5.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gulp": "^4.0.2",
    "onchange": "^6.1.0",
    "ospec": "^4.1.6",
    "prettier": "^2.6.0",
    "typescript": "^4.8.3"
  },
  "hxmstyle": {
    "options": {
      "typescript": true
    },
    "dependenciesAdded": [
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "eslint",
      "eslint-config-prettier",
      "eslint-plugin-deprecation",
      "eslint-plugin-destructure-depth",
      "eslint-plugin-destructuring",
      "eslint-plugin-import",
      "eslint-plugin-prettier",
      "eslint-plugin-simple-import-sort",
      "eslint-plugin-total-functions",
      "eslint-plugin-unused-imports",
      "prettier"
    ]
  },
  "author": "Hugsmiðjan ehf (http://www.hugsmidjan.is)",
  "contributors": [
    "Már Örlygsson <mar@hugsmidjan.is>"
  ],
  "repository": "git@github.com:hugsmidjan/cssserver.git"
}
